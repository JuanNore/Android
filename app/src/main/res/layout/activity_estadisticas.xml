<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EstadisticasActivity">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="112dp"
        android:text="Estadisticas"
        android:textColor="#0000ff"
        android:textAlignment="center"
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="301dp"
        android:layout_height="55dp"
        android:textSize="17dp"
        android:text="Cantidad de estudiantes procesados: "
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="212dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="301dp"
        android:layout_height="55dp"
        android:textSize="17dp"
        android:text="Cantidad de estudiantes que pierden: "
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="380dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="292dp"
        android:layout_height="64dp"
        android:text="Cantidad de estudiantes que pueden recuperar:"
        android:textSize="17dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="455dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="301dp"
        android:layout_height="55dp"
        android:textSize="17dp"
        android:text="Cantidad de estudiantes que ganan: "
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="297dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textCantProcesados"
        android:layout_width="66dp"
        android:layout_height="53dp"
        android:textSize="17dp"
        android:text=""
        tools:layout_editor_absoluteX="329dp"
        tools:layout_editor_absoluteY="214dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textCantGanan"
        android:layout_width="66dp"
        android:layout_height="53dp"
        android:textSize="17dp"
        android:text=""
        tools:layout_editor_absoluteX="329dp"
        tools:layout_editor_absoluteY="299dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textCantPierden"
        android:layout_width="66dp"
        android:layout_height="53dp"
        android:textSize="17dp"
        android:text=""
        tools:layout_editor_absoluteX="329dp"
        tools:layout_editor_absoluteY="382dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textCantRecuperan"
        android:layout_width="66dp"
        android:layout_height="53dp"
        android:text=""
        android:textSize="17dp"
        tools:layout_editor_absoluteX="172dp"
        tools:layout_editor_absoluteY="538dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_salirEst"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="630dp"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>