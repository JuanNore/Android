<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegistroActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:text="Registro"
        android:textAlignment="center"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/txtNombre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/txtNombre"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nombre:"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="133dp" />

    <EditText
        android:id="@+id/txtDocumento"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="47dp"
        android:ems="10"
        android:inputType="number"
        android:text="Documento:"
        app:layout_constraintTop_toTopOf="@+id/txtNombre"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="14dp" />

    <EditText
        android:id="@+id/txtTelefono"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="47dp"
        android:ems="10"
        android:inputType="phone"
        android:text="Telefono:"
        app:layout_constraintTop_toTopOf="@+id/txtDocumento"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/txtDireccion"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Direccion:"
        app:layout_constraintTop_toTopOf="@+id/txtEdad"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/txtEdad"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        android:ems="10"
        android:inputType="number"
        android:text="Edad:"
        app:layout_constraintTop_toTopOf="@+id/txtTelefono"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/txtMateria1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="389dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/txtMateria2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="434dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/txtMateria3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="479dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/txtMateria5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="569dp" />

    <EditText
        android:id="@+id/txtMateria4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="524dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/txtNota2"
        android:layout_width="157dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="number"
        android:text="Nota 2:"
        tools:layout_editor_absoluteX="254dp"
        tools:layout_editor_absoluteY="434dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/txtNota3"
        android:layout_width="157dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="number"
        android:text="Nota 3:"
        tools:layout_editor_absoluteX="254dp"
        tools:layout_editor_absoluteY="477dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/txtNota4"
        android:layout_width="157dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="number"
        android:text="Nota 4:"
        tools:layout_editor_absoluteX="254dp"
        tools:layout_editor_absoluteY="520dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/txtNota5"
        android:layout_width="157dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="number"
        android:text="Nota 5"
        tools:layout_editor_absoluteX="254dp"
        tools:layout_editor_absoluteY="563dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/txtNota1"
        android:layout_width="157dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="number"
        android:text="Nota 1:"
        tools:layout_editor_absoluteX="254dp"
        tools:layout_editor_absoluteY="389dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_registrar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="302dp"
        tools:layout_editor_absoluteY="638dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_salir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="638dp"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>